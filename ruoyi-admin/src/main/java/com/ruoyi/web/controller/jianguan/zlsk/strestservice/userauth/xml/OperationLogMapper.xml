<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.ruoyi.web.controller.jianguan.zlsk.strestservice.mapper.OperationLogMapper">

    <select id="select" resultType="Map" parameterType="Map">
        select * from ss_f_operationlog as t1
        left join ss_f_users as t2 on t1.USERID = t2.ID
        where 1=1
        order by t1.STTIME desc
    </select>

    <insert id="insert" parameterType="Map">
       INSERT INTO ss_f_operationlog (USERID,IP,Operation) VALUES(#{userid},#{ip},#{Operation})
    </insert>
</mapper>
